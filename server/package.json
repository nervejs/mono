{
	"name": "@nervejs/server",
	"version": "2.0.0",
	"description": "node server for nervejs framework",
	"main": "dist/index",
	"typings": "dist/index",
	"scripts": {
		"clean": "rm -rf ./dist",
		"type-check": "./node_modules/.bin/tsc --emitDeclarationOnly",
		"type-check:watch": "npm run type-check -- --watch",
		"dev": "npm run build && webpack --mode=development --watch",
		"start": "./bin/run.sh",
		"retire": "retire",
		"eslint": "eslint 'src/**/*.{ts,tsx}' --cache",
		"check": "npm run eslint && npm run retire",
		"build": "npm run clean && npm run type-check && webpack --mode=production",
		"prepublishOnly": "npm run build && npm run check && bump"
	},
	"dependencies": {
		"browser-detect": "^0.2.28",
		"commander": "^5.1.0",
		"cookie-parser": "^1.4.5",
		"deepmerge": "^4.2.2",
		"express": "^4.17.1",
		"gettext-parser": "^4.0.4",
		"handlebars": "^4.7.6",
		"request": "^2.88.2",
		"uuid": "^8.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.10.5",
		"@babel/preset-env": "^7.10.4",
		"@nervejs/eslint-plugin": "^1.0.0",
		"@types/cookie-parser": "^1.4.2",
		"@types/express": "^4.17.7",
		"@types/node": "^14.0.23",
		"@types/request": "^2.48.5",
		"@types/uuid": "^8.0.0",
		"babel-loader": "^8.1.0",
		"circular-dependency-plugin": "^5.2.0",
		"retire": "^2.2.1",
		"thread-loader": "^2.1.3",
		"ts-loader": "^8.0.1",
		"typescript": "^3.9.7",
		"uglifyjs-webpack-plugin": "^2.2.0",
		"version-bump-prompt": "^6.0.5",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.12"
	},
	"author": "denis.yarkeev@gmail.com",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nervejs/node-nerve.git"
	},
	"keywords": [
		"framework",
		"nervejs",
		"nerve",
		"rendering"
	]
}
